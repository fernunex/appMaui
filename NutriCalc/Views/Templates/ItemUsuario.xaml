<?xml version="1.0" encoding="utf-8" ?>
<ContentView xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="NutriCalc.Views.Templates.ItemUsuario"
             xmlns:models="clr-namespace:NutriCalc.Models">
    <HorizontalStackLayout
        x:DataType="models:UsuarioModel"
        Padding="2" BackgroundColor="Beige">
        <Label Text="{Binding Nombre}"></Label>
        <Label Text="{Binding Apellido}"></Label>
    </HorizontalStackLayout>
</ContentView>