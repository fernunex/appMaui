<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="NutriCalc.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:NutriCalc"
    xmlns:views="clr-namespace:NutriCalc.Views"
    Shell.FlyoutBehavior="Flyout"
    Title="NutriCalc">

    <!--<ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate views:UsuarioRegistro}"
        Route="MainPage" />-->

    <ShellContent
        Title="Listado Usuarios"
        ContentTemplate="{DataTemplate views:ListadoUsuarios}"
        Route="ListadoUsuarios" />
    <ShellContent
        Title="Registro Usuario"
        ContentTemplate="{DataTemplate views:UsuarioRegistro}"
        Route="UsuarioRegistro" />
    <ShellContent
        Title="Detalle Usuario"
        ContentTemplate="{DataTemplate views:DetalleUsuario}"
        Route="DetalleUsuario" />

</Shell>
